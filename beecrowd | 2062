#include <stdio.h>
int main()
{
    unsigned short number, ok, red;
    char reduan[30];

    scanf("%hu", &number);

    for (int i = 0; i < number; i++)
    {

        scanf("%s", reduan);
        red = strlen(reduan);

        if (i != 0)
        {
            printf(" ");
        }

        if (red == 3 && reduan[0] == 'O' && reduan[1] == 'B')
        {
            printf("OBI");
        }

        else if (red == 3 && reduan[0] == 'U' && reduan[1] == 'R')
        {
            printf("URI");
        }

        else{
            printf("%s",reduan);
        }
          
    }

    printf("\n");
}
